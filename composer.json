{
  "name": "bronto/bronto-php-common",
  "keywords": ["bronto", "php", "common"],
  "homepage": "http://dev/bronto.com",
  "license": "Apache-2.0",
  "version": "1.0.0",
  "require-dev": {
    "phpunit/phpunit": "4.5.*"
  },
  "autoload": {
    "psr-4": {
      "Bronto\\": "src/Bronto"
    },
    "psr-0": {
      "Bronto_": "legacy/src"
    },
    "files": [
      "src/Bronto/functions.php",
      "src/Bronto/Functional/functions.php"
    ]
  },
  "bin": ["bin/bronto-legacy"],
  "scripts": {
    "test": "phpunit",
    "test-legacy": "phpunit --configuration legacy-phpunit.xml.dist",
    "legacy": "php bin/bronto-legacy"
  }
}
